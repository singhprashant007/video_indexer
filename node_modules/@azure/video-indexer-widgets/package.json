{
  "name": "@azure/video-indexer-widgets",
  "version": "1.0.5",
  "description": "A package by Video Indexer in order to embed Video Indexer insights widget in the customer service and customize it's style and data.",
  "main": "./dist/index.min.js",
  "typings": "./dist/npm.definitions.d.ts",
  "scripts": {
    "start": "webpack -w",
    "build": "webpack --mode=development",
    "build:prod": "webpack --mode=production",
    "update-version": "npm version",
    "bump-version": "npm version patch",
    "lint": "npx eslint . --ext .js,.jsx,.ts,.tsx",
    "schema:vi-insights-widget": "typescript-json-schema .\\src\\widgets\\insights\\vi-insights\\definitions\\vi-widget-config.definitions.ts IInsightsWidgetConfig -o .\\src\\assets\\vi-widget-config.schema.json --required --refs=false",
    "schema:custom-insights-widget": "typescript-json-schema .\\src\\widgets\\insights\\custom-insights\\definitions\\custom-insights-config.definitions.ts ICustomInsightsWidgetConfig -o .\\src\\assets\\custom-insights-config.schema.json --required --refs=false",
    "test": "tsc --p tsconfig.spec.json && wtr --node-resolve",
    "test:coverage": "npm run test -- --coverage",
    "test:watch": "npm run test -- --watch --preserveWatchOutput",
    "feed:auth": "npx vsts-npm-auth -config .npmrc"
  },
  "keywords": [
    "Video Indexer",
    "widgets",
    "azure",
    "video analyzer",
    "media"
  ],
  "author": "Microsoft Azure",
  "license": "MIT",
  "devDependencies": {
    "@open-wc/testing": "^3.0.0-next.2",
    "@types/eslint": "^7.28.0",
    "@types/estree": "0.0.50",
    "@typescript-eslint/eslint-plugin": "^4.29.1",
    "@typescript-eslint/parser": "^4.29.1",
    "@web/test-runner": "^0.13.16",
    "@web/test-runner-playwright": "^0.8.8",
    "copy-webpack-plugin": "^9.0.1",
    "eslint": "^7.32.0",
    "eslint-plugin-security": "^1.4.0",
    "eslint-webpack-plugin": "^3.0.1",
    "ts-loader": "^8.0.11",
    "tslint": "^6.1.3",
    "tslint-loader": "^3.5.4",
    "typescript": "^4.0.5",
    "typescript-json-schema": "^0.48.0",
    "webpack": "^5.5.1",
    "webpack-cli": "^4.2.0",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {},
  "files": [
    "dist",
    "LICENSE"
  ],
  "homepage": "https://www.videoindexer.ai",
  "bugs": {
    "url": "https://github.com/Azure-Samples/media-services-video-indexer/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Azure-Samples/media-services-video-indexer.git"
  },
  "sideEffects": false
}
